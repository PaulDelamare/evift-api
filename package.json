    "@prisma/client": "5.15.0",
    "elysia": "latest",
    "elysia-rate-limit": "^4.1.0",
    "handlebars": "^4.7.8",
    "mongoose": "^8.4.5",
    "nodemailer": "^6.9.13",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/nodemailer": "^6.4.15",
    "@types/pg": "^8.11.6",
    "bun-types": "latest",
    "prisma": "^5.15.0"
  },
  "module": "src/index.js",
  "bun-create": {
    "start": "bun run src/index.ts"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
